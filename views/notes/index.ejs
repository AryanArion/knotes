<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head.ejs') %>
<body>
    <%- include('../partials/nav.ejs') %>
    <div class="outer" id="content_wrap">
        <section class="inner">
            <h1>Notatki</h1>
            <% if(notes.length > 0) { %>
                <% notes.forEach(note => { %>
                    <a href="/notes/<%= note %>"><%= note %></a>
                    <br>
                <% }) %>
            <% } else { %>
                <p>Nie ma notatek!</p>
            <% } %>
        </section>
    </div>
    <%- include('../partials/footer.ejs') %>
</body>
</html>